{
    "name": "denisyukphp/tmpfile-manager",
    "description": "Temp files manager",
    "keywords": ["tmpfile", "tempnam", "temp", "tmp", "file", "manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Denisyuk",
            "email": "a@denisyuk.by",
            "homepage": "https://denisyuk.by/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "denisyukphp/tmpfile": "^2.3",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/filesystem": "^4.4 || ^5.0",
        "symfony/process": "^4.4 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "TmpFileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TmpFileManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
