{
    "name": "denisyukphp/tmpfile-manager",
    "description": "Temp files manager",
    "keywords": ["tmpfile", "tempnam", "temp", "tmp", "file", "manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Denisyuk",
            "email": "a@denisyuk.by",
            "homepage": "https://denisyuk.by/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "denisyukphp/tmpfile": "^3.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TmpFileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TmpFileManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
